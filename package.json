{
  "name": "sftp-rsync-fast",
  "displayName": "sftp-rsync-fast",
  "description": "",
  "version": "0.0.12",
  "publisher": "flyhope",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/flyhope/sftp-rsync.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.syncProject",
        "title": "sftp-rsync: Rsync upload project"
      }
    ],
    "configuration": {
        "properties": {
            "sftp-rsync-fast.allowFileAccess": {
                "type": "boolean",
                "default": true,
                "description": "Allow the extension to access files on disk."
            }
        }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
